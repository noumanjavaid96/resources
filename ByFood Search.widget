{
  "version": "1.0",
  "name": "ByFood Search",
  "template": "{\"type\":\"Card\",\"size\":\"sm\",\"children\":[{\"type\":\"Col\",\"gap\":2,\"children\":[{\"type\":\"Title\",\"value\":{{ (heading) | tojson }},\"size\":\"md\"},{\"type\":\"Text\",\"value\":{{ (tagline) | tojson }},\"size\":\"sm\",\"color\":\"secondary\"}]},{\"type\":\"Divider\"},{\"type\":\"Form\",\"onSubmitAction\":{\"type\":\"byfood.search\"},\"children\":[{\"type\":\"Col\",\"gap\":3,\"children\":[{\"type\":\"Input\",\"name\":\"query.location\",\"placeholder\":\"City or area\",\"defaultValue\":{{ (defaultLocation) | tojson }}},{\"type\":\"Row\",\"gap\":3,\"children\":[{\"type\":\"DatePicker\",\"name\":\"query.date\",\"defaultValue\":{{ (defaultDate) | tojson }}},{\"type\":\"Select\",\"name\":\"query.category\",\"options\":{{ (categoryOptions) | tojson }},\"defaultValue\":{{ (defaultCategory) | tojson }}}]},{\"type\":\"Row\",\"gap\":3,\"children\":[{\"type\":\"Select\",\"name\":\"query.guests\",\"options\":{{ (guestOptions) | tojson }},\"defaultValue\":{{ (defaultGuests) | tojson }}},{\"type\":\"Spacer\"},{\"type\":\"Button\",\"submit\":true,\"label\":\"Search byFood\",\"style\":\"primary\"}]}]}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "heading": {
        "type": "string"
      },
      "tagline": {
        "type": "string"
      },
      "defaultDate": {
        "type": "string"
      },
      "defaultLocation": {
        "type": "string"
      },
      "categoryOptions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string"
            },
            "value": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          },
          "required": [
            "label",
            "value"
          ],
          "additionalProperties": false
        }
      },
      "defaultCategory": {
        "type": "string"
      },
      "guestOptions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string"
            },
            "value": {
              "type": "string"
            },
            "disabled": {
              "type": "boolean"
            }
          },
          "required": [
            "label",
            "value"
          ],
          "additionalProperties": false
        }
      },
      "defaultGuests": {
        "type": "string"
      }
    },
    "required": [
      "heading",
      "tagline",
      "defaultDate",
      "defaultLocation",
      "categoryOptions",
      "defaultCategory",
      "guestOptions",
      "defaultGuests"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "sm",
    "children": [
      {
        "type": "Col",
        "gap": 2,
        "children": [
          {
            "type": "Title",
            "value": "byFood Japan",
            "size": "md"
          },
          {
            "type": "Text",
            "value": "Find tours, classes, and dining",
            "size": "sm",
            "color": "secondary"
          }
        ]
      },
      {
        "type": "Divider"
      },
      {
        "type": "Form",
        "onSubmitAction": {
          "type": "byfood.search"
        },
        "children": [
          {
            "type": "Col",
            "gap": 3,
            "children": [
              {
                "type": "Input",
                "name": "query.location",
                "placeholder": "City or area",
                "defaultValue": "Tokyo"
              },
              {
                "type": "Row",
                "gap": 3,
                "children": [
                  {
                    "type": "DatePicker",
                    "name": "query.date",
                    "defaultValue": "2025-11-12"
                  },
                  {
                    "type": "Select",
                    "name": "query.category",
                    "options": [
                      {
                        "value": "tours",
                        "label": "Food tours"
                      },
                      {
                        "value": "classes",
                        "label": "Cooking classes"
                      },
                      {
                        "value": "restaurants",
                        "label": "Restaurants"
                      },
                      {
                        "value": "tea",
                        "label": "Tea ceremony"
                      }
                    ],
                    "defaultValue": "tours"
                  }
                ]
              },
              {
                "type": "Row",
                "gap": 3,
                "children": [
                  {
                    "type": "Select",
                    "name": "query.guests",
                    "options": [
                      {
                        "value": "1",
                        "label": "1"
                      },
                      {
                        "value": "2",
                        "label": "2"
                      },
                      {
                        "value": "3",
                        "label": "3"
                      },
                      {
                        "value": "4",
                        "label": "4"
                      },
                      {
                        "value": "5",
                        "label": "5"
                      },
                      {
                        "value": "6",
                        "label": "6"
                      }
                    ],
                    "defaultValue": "2"
                  },
                  {
                    "type": "Spacer"
                  },
                  {
                    "type": "Button",
                    "submit": true,
                    "label": "Search byFood",
                    "style": "primary"
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
